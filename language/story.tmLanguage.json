{
	"name": "Story",
	"scopeName": "source.story",
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"patterns": [
		{
			"include": "#styles"
		}
	],
	"repository": {
		"styles": {
			"match": "^(`)(.*)(`)$",
			"captures": {
				"1": {
					"name": "entity.name.class.story"
				},
				"3": {
					"name": "entity.name.class.story"
				}
			}
		}
	}
}
